
/*

For each view, provide a function(params, includeObject, doneCb, cbs)
*/

function computeViewFunction(schema, viewSchema){
	function f(params, includeObject, doneCb, cbs){
	}
	
	return f;
}

function computeViewFunctions(schema){
	var vf = {};
	_.each(schema, function(v){
		if(v.isView){
			vf[v.code] = computeViewFunction(schema, v);
		}
	});
	
	return vf;
}

